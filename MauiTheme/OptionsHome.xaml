<?xml version="1.0" encoding="utf-8" ?>
<b:BaseContentPage
    x:Class="MauiTheme.OptionsHome"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:b="clr-namespace:MauiTheme"
    xmlns:t="http://schemas.telerik.com/2022/xaml/maui"
    xmlns:vm="clr-namespace:MauiTheme.ViewModel"
    Title="Options"
    x:DataType="vm:OptionsViewModel">
    <VerticalStackLayout>
        <Button Command="{Binding GotoThirdPageCommand}" Text="Go to third page" />

        <t:RadListPicker ItemsSource="{Binding MyItems}" />
    </VerticalStackLayout>
</b:BaseContentPage>